{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 60
    },
    "./src/lib/": {
      "branches": 90,
      "statements": 90
    }
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
